    <webLinks>
        <fullName>Accept_Edit</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Accept &amp; Edit</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/connection.js&quot;)} //adds the proper code for inclusion of AJAX toolkit
var url = parent.location.href; //string for the URL of the current page
var editpageurl=&apos;/apex/AP_TrackerEdit?access=true&amp;trackerid=&apos;;
var records = {!GETRECORDIDS($ObjectType.AP_Tracker__c)}; //grabs the Lead records that the user is requesting to update
var updateRecords = []; //array for holding records that this code will ultimately update

if (records[0] == null) { //if the button was clicked but there was no record selected
	alert(&quot;Please select at least one record to update.&quot;); //alert the user that they didn&apos;t make a selection 
} else { //otherwise, there was a record selection
	if(records.length&gt;1){
		alert(&quot;Please select only one record to update.&quot;);
	}else{
		var trkId = records[0];
		var qr = sforce.connection.query(&quot;SELECT id,name,akritivtlm__Current_State__c,akritivtlm__Tracker_Id__c FROM akritivtlm__AP_Tracker__c where id=&apos;&quot;+ trkId +&quot;&apos;&quot;);
		
		if (qr.size == 0)
		{
			alert(&quot;There is some issue.Please try again.&quot;);
			//return;
		}
		else
		{
			
			//get tracker record
			var parentId = qr.records.akritivtlm__Tracker_Id__c;
			var qrTrk = sforce.connection.query(&quot;SELECT id,name,akritivtlm__Processor_Name__c,akritivtlm__Rejector_Name__c FROM akritivtlm__Tracker__c where id=&apos;&quot;+ parentId +&quot;&apos;&quot;);
			
			if(qrTrk.size == 0) {
				alert(&quot;There is some issue.Please try again.&quot;);
			} else {
				
				var qcRights = true;
				var rejectRights = true;
				var loginUser = &quot;{!$User.FirstName} {!$User.LastName}&quot;;
				
				//for check qc
				var qcStateRec = sforce.connection.query(&quot;SELECT id,Name,akritivtlm__Field_Name__c FROM akritivtlm__APP_Logic__c where Name=&apos;APOBJ.QCState&apos;&quot;);
				if(qcStateRec.size &gt; 0) {
					if(qcStateRec.records.akritivtlm__Field_Name__c != null &amp;&amp; qcStateRec.records.akritivtlm__Field_Name__c != &apos;&apos;) {
						var qcArr=qcStateRec.records.akritivtlm__Field_Name__c.split(&apos;,&apos;);
						
						for(var i=0;i&lt;qcArr.length;i++) {
							if(qcArr[i] == qr.records.akritivtlm__Current_State__c) {
								if(loginUser == qrTrk.records.akritivtlm__Processor_Name__c) {
									qcRights = false;
									break;
								}
							}
						}
					}
				}
				//for check qc
				
				//for check reject
				var rejectStateRec = sforce.connection.query(&quot;SELECT id,Name,akritivtlm__Field_Name__c FROM akritivtlm__APP_Logic__c where Name=&apos;APOBJ.RejectState&apos;&quot;);
				if(rejectStateRec.size &gt; 0) {
					if(rejectStateRec.records.akritivtlm__Field_Name__c != null &amp;&amp; rejectStateRec.records.akritivtlm__Field_Name__c != &apos;&apos;) {
						var rejArr=rejectStateRec.records.akritivtlm__Field_Name__c.split(&apos;,&apos;);
						
						for(var i=0;i&lt;rejArr.length;i++) {
							if(rejArr[i] == qr.records.akritivtlm__Current_State__c) {
								if(loginUser == qrTrk.records.akritivtlm__Rejector_Name__c) {
									rejectRights = false;
									break;
								}
							}
						}
					}
				}
				//for check reject
				
				//now finally redirect
				if(qcRights &amp;&amp; rejectRights) {
					var update_tracker = new sforce.SObject(&quot;akritivtlm__AP_Tracker__c&quot;); //create a new sObject for storing updated record details
					update_tracker.Id = records[0]; //set the Id of the selected Lead record
					update_tracker.ownerid= &quot;{!$User.Id}&quot;; //set the value for OwnerId to Current User
					updateRecords.push(update_tracker); //add the updated record to our array
					result = sforce.connection.update(updateRecords); //push the updated records back to Salesforce
					parent.location.href = editpageurl+records[0];
				} else if (!qcRights) {
					alert(&apos;you dont have permission for QC&apos;);
				} else if (!rejectRights) {
					alert(&apos;you dont have permission for Reject&apos;);
				}
				
			}
			
		}
		
		
	}

}</url>
    </webLinks>
